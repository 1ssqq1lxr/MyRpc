        			<rpc:serviceConsume name="Test6Service" interface="Test6Service" ></rpc:serviceConsume>
        			<rpc:serviceConsume name="Test7Service" interface="Test7Service" ></rpc:serviceConsume>
        			<rpc:serviceConsume name="Test8Service" interface="Test8Service" ></rpc:serviceConsume>
